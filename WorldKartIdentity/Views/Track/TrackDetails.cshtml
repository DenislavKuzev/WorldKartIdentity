@model WorldKartIdentity.ViewModel.TrackViewModel

@{
    ViewData["Title"] = "TrackDetails";
    Layout = "~/Views/Shared/_NewLayout.cshtml";
}

@section Styles {
    <link rel="stylesheet" href="~/css/trackdetails.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/pintura/pintura.css" />
    <link rel="stylesheet" href="~/lib/node_modules/@@recogito/annotorious/dist/annotorious.min.css" />
}

<section class="breadcrumb-section set-bg" style="background-image:url('/img/trackbackground.png');
                background-size:cover;
                background-position:center;
                background-repeat:no-repeat;">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <div class="breadcrumb-text">
                    <h2>@Model.Name</h2>
                </div>
            </div>
        </div>
        <a class="primary-btn btn-normal appoinment-btn" asp-action="TrackGallery" asp-controller="Track">НАЗАД</a>
    </div>
</section>

<section class="class-details-section description">
    <div class="container">
        <div class="row">
            <div class="col-lg-8">
                <div class="class-details-text">
                    <div class="cd-text">
                        <div class="cd-single-item">
                            <h3>Описание</h3>
                            <p>
                                @Model.Description
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

@if (User.Identity.IsAuthenticated == true)
{
    <section class="class-details-section spad">
        <div class="container d-flex align-items-center justify-content-center">
            <div class="row img-container d-flex w-75 h-75">
                <div class="col-lg-10 pnt-editor">
                </div>
            </div>
        </div>
    </section>
}

<section class="team-section team-page spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="team-title">
                    <div class="section-title">
                        <span>Траектории</span>
                        <h2>НАЙ-ХАРЕСВАНИ ТРАЕКТОРИИ</h2>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-4 col-sm-6">
                <div class="ts-item set-bg" style="background-image: url('data:image/webp;base64,');">
                    <div class="ts_text">
                        <h4>Added by:</h4>
                        <span>Number of likes</span>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-sm-6">
                <div class="ts-item set-bg" style="background-image: url('data:image/webp;base64,');">
                    <div class="ts_text">
                        <h4>Added by:</h4>
                        <span>Number of likes</span>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-sm-6">
                <div class="ts-item set-bg" style="background-image: url('data:image/webp;base64,');">
                    <div class="ts_text">
                        <h4>Added by:</h4>
                        <span>Number of likes</span>
                    </div>
                </div>
            </div>
        </div>
        <a href="#" class="primary-btn btn-normal appoinment-btn">ВСИЧКИ ТРАЕКТОРИИ</a>
    </div>
</section>

<section class="contact-section spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-6 col-sm-6">
                <div class="section-title contact-title">
                    <span>Данни на пистата</span>
                    <h2>СВЪРЗВАНЕ</h2>
                </div>
                <div class="contact-widget">
                    <div class="cw-text">
                        <i class="fa fa-map-marker"></i>
                        <p>@Model.Location</p>
                    </div>
                    <div class="cw-text">
                        <i class="fa fa-mobile"></i>
                        <ul>
                            <li>@Model.TelNumber</li>
                        </ul>
                    </div>
                    <div class="cw-text email">
                        <i class="fa fa-envelope"></i>
                        <p>@Model.Email</p>
                    </div>
                    <div class="cw-text">
                        <i class="fa fa-clock-o" aria-hidden="true"></i>
                        <ul>
                            <li>@Model.Worktime</li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="col-lg-6 col-sm-8">
                <div class="map">
                    <iframe src="@Model.GoogleMapsLink"
                            height="450" style="border:0;" allowfullscreen="">
                    </iframe>
                </div>
            </div>
        </div>
    </div>
</section>

@section Scripts {
    <script>
        window.trackContext = {
            trackId: @Model.Id
        };
    </script>

    <script type="module" src="~/lib/pintura/pintura.js"></script>
    <script type="module" src="~/js/trackdetails.js"></script>
    <script src="~/lib/node_modules/@@recogito/annotorious/dist/annotorious.min.js"></script>

}