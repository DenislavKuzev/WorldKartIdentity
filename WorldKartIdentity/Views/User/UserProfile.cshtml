@model WorldKartIdentity.ViewModel.UserViewModel

@{
    ViewData["Title"] = "UserProfile";
    Layout = "~/Views/Shared/_NewLayout.cshtml";
}

@section Styles {
    <link rel="stylesheet" href="~/css/userprofile.css" asp-append-version="true" />
}

<section class="class-details-section spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-10">
                <a asp-action="EditUserProfile" asp-controller="User" class="primary-btn btn-normal appoinment-btn">РЕДАКТИРАНЕ</a>
                <div class="class-details-text">
                    <div class="cd-trainer">
                        <div class="row">
                            <div class="col-md-5">
                                <div class="cd-trainer-pic">
                                    @if (!string.IsNullOrEmpty(Model.Picture))
                                    {
                                        <img src="@Model.Picture" alt="Profile picture" />
                                    }
                                    else
                                    {
                                        <img src="~/img/defaultprofilepic.png" alt="Default picture" />
                                    }
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="cd-trainer-text">
                                    <div class="trainer-title">
                                        <h4>@Model.UserName</h4>
                                        <span>Няма добавена роля</span>
                                    </div>
                                    <div class="trainer-social">
                                        <div class="trainer-title">
                                            <span>Социални мрежи</span>
                                        </div> <div class="form-group trainer-row">
                                            <label><a><i class="fa fa-facebook"></i></a></label>
                                            <label><a><i class="fa fa-twitter"></i></a></label>
                                            <label><a><i class="fa fa-youtube-play"></i></a></label>
                                            <label><a> <i class="fa fa-instagram"></i></a></label>
                                        </div>
                                    </div><div class="trainer-title">
                                        <h4>Моите данни</h4>
                                        <ul class="trainer-info">
                                            <li><span>Имейл</span>@Model.Email</li>
                                            <li class="trainer-row">
                                                <span>Телефон</span>
                                                <span>@Model.PhoneNumber</span>
                                            </li>
                                            <li class="trainer-row">
                                                <span>Държава</span>
                                                <span>@Model.Country</span>
                                            </li>
                                        </ul>
                                        <p>
                                            @Model.Bio
                                        </p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="team-section team-page spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="team-title">
                    <div class="section-title">
                        <span>Моите Треактории</span>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-4 col-sm-6">
                <div class="ts-item set-bg" style="background-image: url('data:image/webp;base64,');">
                    <div class="ts_text">
                        <h4>Added by:</h4>
                        <span>Number of likes</span>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-sm-6">
                <div class="ts-item set-bg" style="background-image: url('data:image/webp;base64,');">
                    <div class="ts_text">
                        <h4>Added by:</h4>
                        <span>Number of likes</span>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-sm-6">
                <div class="ts-item set-bg" style="background-image: url('data:image/webp;base64,');">
                    <div class="ts_text">
                        <h4>Added by:</h4>
                        <span>Number of likes</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<section class="team-section team-page spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-10">
                <div class="team-title">
                    <div class="section-title">
                        <span>Моите Постове</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>