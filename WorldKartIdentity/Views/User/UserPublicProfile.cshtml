@model WorldKartIdentity.ViewModel.UserViewModel

@{
    ViewData["Title"] = "UserPublicProfile";
    Layout = "~/Views/Shared/_NewLayout.cshtml";
}

@section Styles {
    <link rel="stylesheet" href="~/css/userprofile.css" asp-append-version="true" />
}

<section class="class-details-section spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-10">
                <div class="class-details-text">
                    <div class="cd-text">
                        <div class="cd-single-item">
                            <h3>Потребителски профил</h3>
                        </div>
                    </div>
                    <div class="cd-trainer">
                        <div class="row">
                            <div class="col-md-5">
                                <div class="cd-trainer-pic">
                                    @if (!string.IsNullOrEmpty(Model.Picture))
                                    {
                                        <img src="@Model.Picture" alt="Profile picture" />
                                    }
                                    else
                                    {
                                        <img src="~/img/defaultprofilepic.png" alt="Default picture" />
                                    }
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="cd-trainer-text">
                                    <div class="trainer-title">
                                        <h4>@Model.UserName</h4>
                                        <span>@Model.RoleInKarting</span>
                                    </div>
                                    <div class="trainer-social">
                                        <div class="trainer-title">
                                            <span>Социални мрежи</span>
                                        </div>
                                        <a href="#"><i class="fa fa-facebook"></i></a>
                                        <a href="#"><i class="fa fa-twitter"></i></a>
                                        <a href="#"><i class="fa fa-youtube-play"></i></a>
                                        <a href="#"><i class="fa fa-instagram"></i></a>
                                        <a href="#"><i class="fa  fa-envelope-o"></i></a>
                                    </div><div class="trainer-title">
                                        <h4>Данни</h4>
                                    </div>
                                    <ul class="trainer-info">
                                        <li><span>Имейл</span>@Model.Email</li>
                                        <li><span>Държава</span>@Model.Country</li>
                                    </ul>
                                    <p>
                                        @Model.Bio
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="team-section team-page spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="team-title">
                    <div class="section-title">
                        <span>Траектории на @Model.UserName</span>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-4 col-sm-6">
                <div class="ts-item set-bg" style="background-image: url('data:image/webp;base64,');">
                    <div class="ts_text">
                        <h4>Added by:</h4>
                        <span>Number of likes</span>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-sm-6">
                <div class="ts-item set-bg" style="background-image: url('data:image/webp;base64,');">
                    <div class="ts_text">
                        <h4>Added by:</h4>
                        <span>Number of likes</span>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-sm-6">
                <div class="ts-item set-bg" style="background-image: url('data:image/webp;base64,');">
                    <div class="ts_text">
                        <h4>Added by:</h4>
                        <span>Number of likes</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<section class="team-section team-page spad">
    <div class="container">
        <div class="row">
            <div class="col-lg-10">
                <div class="team-title">
                    <div class="section-title">
                        <span>Постове на @Model.UserName</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>